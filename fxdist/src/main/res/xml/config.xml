<?xml version='1.0' encoding='utf-8'?><!--suppress XmlUnusedNamespaceDeclaration -->

<widget xmlns:cdv="http://cordova.apache.org/ns/1.0"
    id="kr.fluxion"
    version="1.0.0"
    xmlns="http://www.w3.org/ns/widgets">
    <name>fx</name>
    <description></description>
    <author
        email="fluxionsoft@naver.com"
        href="https://fluxion.kr">
    </author>
    <!-- 운영 -->
    <content src="http://fluxion.kr:8070" />

    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />

    <preference name="loglevel" value="DEBUG" />
    <preference name="GradlePluginKotlinEnabled" value="true" />
    <preference name="GradlePluginKotlinCodeStyle" value="official" />
    <preference name="GradlePluginKotlinVersion" value="1.4.10" />
    <preference name="AndroidXEnabled" value="true" />

    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Navigation">
        <param name="android-package" value="kr.fluxion.cordova.plugin.navigation.Navigation" />
    </feature>
    <feature name="NFC">
        <param name="android-package" value="kr.fluxion.cordova.plugin.nfc.NFC" />
    </feature>
    <feature name="SignPad">
        <param name="android-package" value="kr.fluxion.cordova.plugin.signpad.SignPad" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="kr.fluxion.cordova.plugin.camera.Camera" />
    </feature>
    <feature name="Location">
        <param name="android-package" value="kr.fluxion.cordova.plugin.location.Location" />
    </feature>
    <feature name="Barcode">
        <param name="android-package" value="kr.fluxion.cordova.plugin.barcode.Barcode" />
    </feature>
    <feature name="Auth">
        <param name="android-package" value="kr.fluxion.cordova.plugin.auth.Auth" />
    </feature>
</widget>